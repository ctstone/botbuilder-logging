{
  "name": "botbuilder-logging",
  "version": "4.0.0-preview.13",
  "description": "Logging middleware for botbuilder. Uses DocumentDb and Azure Blob to persist bot events.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "mocha test/dist/test/spec",
    "prepublishOnly": "npm test && npm version prerelease && npm run build",
    "pretest": "pushd test && tsc",
    "docs": "npm run spell && npm run md",
    "md": "remark . --output",
    "spell": "mdspell \"**/*.md\" \"!node_modules/**\" -r -n -a -x --en-us"
  },
  "publishConfig": {
    "tag": "preview"
  },
  "keywords": [],
  "author": "chstone@microsoft.com",
  "repository": {
    "url": "https://github.com/ctstone/botbuilder-logging",
    "type": "git"
  },
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.0",
    "documentdb": "^1.14.2",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2"
  },
  "peerDependencies": {
    "azure-storage": "^2.8.0",
    "documentdb": "^1.14.2",
    "botbuilder": "^4.0.0-preview1.2"
  },
  "devDependencies": {
    "@types/async": "^2.0.47",
    "@types/chai": "^4.1.2",
    "@types/documentdb": "^1.10.4",
    "@types/lodash.get": "^4.4.3",
    "@types/lodash.set": "^4.3.3",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.0",
    "@types/sinon": "^4.1.3",
    "azure-storage": "^2.8.0",
    "botbuilder": "^4.0.0-preview1.2",
    "chai": "^4.1.2",
    "markdown-spellcheck": "^1.3.1",
    "mocha": "^5.0.0",
    "nock": "^9.1.6",
    "remark-cli": "^5.0.0",
    "remark-lint": "^6.0.2",
    "remark-preset-lint-recommended": "^3.0.2",
    "remark-toc": "^5.0.0",
    "sinon": "^4.2.2",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "remarkConfig": {
    "plugins": [
      "preset-lint-recommended",
      [
        "toc",
        {
          "tight": true,
          "maxDepth": 3
        }
      ]
    ]
  }
}
